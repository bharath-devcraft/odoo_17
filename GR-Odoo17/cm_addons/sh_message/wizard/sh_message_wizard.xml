<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sh_message_wizard" model="ir.ui.view">
        <field name="name">sh.message.wizard</field>
        <field name="model">sh.message.wizard</field>
        <field name="arch" type="xml">
            <form string="Message">
                <script type="text/javascript" src="/sh_message/static/src/js/custom_script.js"/>
                <field name="name" />

                <footer>
                    <button name="entry_confirm" string="Proceed" type="object"  class="oe_highlight"/>
					<button name="entry_cancel" string="Skip" type="object"  class="oe_highlight"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
