<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="seq_ct_quotations" model="ir.sequence">
        <field name="name">Quotations</field>
        <field name="code">ct.quatations</field>
        <field name="prefix">SQ/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_ct_quotations_draft" model="ir.sequence">
        <field name="name">Quotations Draft</field>
        <field name="code">ct.quatations.draft</field>
        <field name="prefix">SQD/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
