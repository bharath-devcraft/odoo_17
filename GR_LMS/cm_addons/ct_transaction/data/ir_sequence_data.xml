<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="seq_ct_transaction" model="ir.sequence">
        <field name="name">CT Transaction</field>
        <field name="code">ct.transaction</field>
        <field name="prefix">CT/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <record id="seq_ct_transaction_draft" model="ir.sequence">
        <field name="name">CT Transaction Draft</field>
        <field name="code">ct.transaction.draft</field>
        <field name="prefix">CTD/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
