		<parameter name="ct_line_id" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select

pro_temp.name  as product_name,
pro_uom.name as product_uom,
ct_line.unit_price,
ct_line.qty

from ct_transaction_line ct_line


left join product_product as product on (product.id = ct_line.product_id)

left join product_template pro_temp on (pro_temp.id = product.product_tmpl_id)

left join uom_uom pro_uom on (pro_uom.id = ct_line.uom_id)



where ct_line.header_id = $P{ct_line_id} order by pro_temp.name]]>
		</queryString>
		<field name="product_name" class="java.lang.Object"/>
		<field name="product_uom" class="java.lang.Object"/>
		<field name="unit_price" class="java.lang.Double"/>
		<field name="qty" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="ct_id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select ct.id as id,
comp.name as company_name,
div_master.name as division_name,

COALESCE(part.street,'')||COALESCE(part.street2,'')||COALESCE(part.name,'')||' - '||COALESCE(part.zip,'')||', '||COALESCE(part.name,'')||', '||COALESCE(part.name,'')||E'\n'||'Phone :  '||COALESCE(part.phone,'')||'   Fax :  '||COALESCE(part.name,'')||E'\n'||'Email :  '||COALESCE(part.email,'')||'    Website :  '||COALESCE(part.website,'') as company_add,
ct.net_amt as net_amount






from ct_transaction ct

left join res_company comp on (comp.id = ct.company_id)

left join res_partner part on (part.id = comp.partner_id)

left join cm_master div_master on (div_master.id = ct.division_id)



where

ct.id = $P{ct_id}
and ct.status != 'draft']]>
	</queryString>
